        $("#HoleDepthToDay, #HoleDepthToNight").bind('change',
            function () {
                var HoleDepthFromDay = document.getElementById('HoleDepthFromDay').innerHTML;
                var HoleDepthFromNight = document.getElementById('HoleDepthToDay').value;
                var result = parseFloat(HoleDepthFromNight) - parseFloat(HoleDepthFromDay);
                if (!isNaN(result)) {
                    document.getElementById('MetersDrilledDay').value = result.toFixed(2);
                    document.getElementById('HoleDepthFromNight').value = parseFloat(HoleDepthFromNight).toFixed(2);
                }

                var HoleDepthToNight = document.getElementById('HoleDepthToNight').value;

                if (parseFloat(HoleDepthToNight) == 0) {
                    var result2 = 0;
                } else {
                    result2 = parseFloat(HoleDepthToNight) - parseFloat(HoleDepthFromNight).toFixed(2);
                }

                if (!isNaN(result2) && !isNaN(result)) {
                    document.getElementById('MetersDrilledNight').value = result2.toFixed(2);
                    document.getElementById('HoleDepthFromTotal').value = result + result2;
                    document.getElementById('MetersDrilledTotal').value = result + result2;
                }

                var HoleDepth = document.getElementById("HoleDepth").innerHTML;
                var result3 = (((result + result2) / parseFloat(HoleDepth)) * 100).toFixed(2);
                if (!isNaN(result3)) {
                    document.getElementById('HoleDepthPercentage').value = result3 + " %";
                }

